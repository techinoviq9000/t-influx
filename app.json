{
  "expo": {
    "name": "T-Influx",
    "slug": "T-Influx",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./app/src/assets/ic_launcher.png",
    "splash": {
      "image": "./app/src/assets/splash_custom.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./app/src/assets/ic_launcher_adaptive_fore.png",
        "backgroundColor": "#34D3C2"
      },
      "package": "com.demo.TInflux",
      "versionCode": 2,
      "permissions": [ "CAMERA", "WRITE_EXTERNAL_STORAGE", "CAMERA_ROLL" ]
    },
    "web": {
      "favicon": "./app/src/assets/favicon.png"
    }
  }
}
